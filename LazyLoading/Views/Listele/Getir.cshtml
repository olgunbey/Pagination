@model (List<Urunler> urunler,int countUrl,string hata)


@{
    if(Model.hata!=null)
    {
        <p>@Model.hata</p>
    }
    else
    {
        @foreach (var urunler in Model.urunler)
        {
            <ul class="list-group">
                <li class="list-group-item active">@urunler.ID</li>
                <li class="list-group-item">@urunler.Fiyat</li>
                <li class="list-group-item">@urunler.UrunAdi</li>
            </ul>
        }
        <br />
    }
}

<form asp-action="Getir" asp-controller="Listele" method="post" asp-route-returnUrl="@Model.countUrl" >
    
    <button > >> </button>


</form>
